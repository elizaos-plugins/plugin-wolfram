{
  "name": "@elizaos/plugin-wolfram",
  "version": "1.3.0",
  "description": "Wolfram Alpha and Wolfram Cloud integration plugin for ElizaOS",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts src/",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "elizaos",
    "plugin",
    "wolfram",
    "wolfram-alpha",
    "computation",
    "mathematics",
    "ai"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "elizaOS Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/eliza.git",
    "directory": "plugin-wolfram"
  },
  "bugs": {
    "url": "https://github.com/elizaos/eliza/issues"
  },
  "homepage": "https://github.com/elizaos/eliza#readme",
  "dependencies": {
    "@elizaos/core": "^1.3.2",
    "axios": "^1.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  }
}
